<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>AzureWebSite</WebPublishMethod>
    <SiteUrlToLaunchAfterPublish>https://your-app-name.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>true</LaunchSiteAfterPublish>
    <MSDeployServiceURL>your-app-name.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>YourAppName</DeployIisAppPath>
    <RemoteSite>your-app-name</RemoteSite>
  </PropertyGroup>
</Project>
